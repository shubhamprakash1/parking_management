<div class="row">
<p id="notice"><%= notice %></p>
  <div class="container">
    <h1>Listing Parkings</h1>
      <div class="table-responsive">
        <table class="table table-striped">
          <thead>
            <div class="col-md-12 col-xs-12">
              <tr>
                <th>Name</th>
                <th>Event</th>
                <th>Price</th>
                <th>Total slot</th>
                <th>Vendor</th>
                <th>Map</th>
                <th>Payment url</th>
                <th colspan="3"></th>
              </tr>
            </div>
          </thead>

          <tbody>
            <% @parkings.each do |parking| %>
              <tr>
                <td><%= parking.name %></td>
                <td><%= parking.event_id %></td>
                <td><%= parking.price %></td>
                <td><%= parking.total_slot %></td>
                <td><%= parking.vendor_id %></td>
                <td><%= parking.map %></td>
                <td><%= parking.payment_url %></td>
                <td><%= link_to 'Show', parking, class: "btn btn-default btn-success" %></td>
                <td><%= link_to 'Edit', edit_parking_path(parking), class: "btn btn-default btn-primary" %></td>
                <td><%= link_to 'Destroy', parking, class: "btn btn-default btn-danger", method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    <br>

    <%= link_to 'New Parking', new_parking_path %>
  </div>
</div>
